// Returns status of a Job Lot Number
query stg-usa/job/get_job_lot_status verb=GET {
  description = "Returns status of a Job Lot Number"

  input {
    text jobLotNumber
  }

  stack {
    api.request {
      url = $env.STG_API_BASE + "/status/" + $input.jobLotNumber
      method = "GET"
      headers = []|array_push:"STG-VALIDATION-API-KEY: " + $env.STGVALIDATIONAPIKEY|array_push:"Accept: application/json"
    } as $response
  }

  response = $response
}
