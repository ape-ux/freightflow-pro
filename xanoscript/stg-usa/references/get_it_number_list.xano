// Fetches CFS freight details list via IT Number
query stg-usa/references/get_it_number_list verb=GET {
  description = "Fetches CFS freight details list via IT Number"

  input {
    text itNumber
  }

  stack {
    api.request {
      url = $env.STG_API_BASE + "/itNumberList/" + $input.itNumber
      method = "GET"
      headers = []|array_push:"STG-VALIDATION-API-KEY: " + $env.STGVALIDATIONAPIKEY|array_push:"Accept: application/json"
    } as $response
  }

  response = $response
}
